{
  "name": "ZEN Theme",
  "dark": true,
  "author": "Piotr Zieli≈Ñski",
  "colors": {
    "bgPanel": "#090b10",
    "bgEditor": "#0f111a",
    "textDefault": "#7b83a3",
    "textInactive": "#4b526d",
    "textActive": "#b7bbcd",
    "border": "#000000",
    "primary": "#ffcb6b",
    "secondary": "#4b526d",
    "textBtn": "#ffffff"
  },
  "ui": {
    "*": {
      "background": "bgPanel",
      "foreground": "textDefault",
      "activeBackground": "bgPanel",
      "activeForeground": "textActive",
      "inactiveBackground": "bgPanel",
      "inactiveForeground": "textInactive",
      "hoverBackground": "bgEditor",
      "hoverForeground": "textActive",
      "pressedBackground": "bgPanel",
      "pressedForeground": "textActive",
      "selectionForeground": "textActive",
      "selectionInactiveForeground": "textInactive",
      "borderColor": "border",
      "separatorColor": "border"
    },
    "Borders": {
      "color": "border",
      "ContrastBorderColor": "border"
    },

    "Tree": {
      "selectionBackground": "bgEditor",
      "selectionInactiveBackground": "bgEditor"
    },
    "ToolWindow": {
      "Header": {
        "background": "bgPanel",
        "inactiveBackground": "bgPanel"
      }
    },

    "EditorTabs": {
      "underlinedTabBackground": "bgPanel",
      "underlinedTabForeground": "textActive",
      "underlineHeight": 0
    },

    "ScrollBar": {
      "Transparent": {
        "thumbColor": "bgEditor",
        "trackColor": "bgPanel",
        "hoverThumbColor": "bgEditor",
        "hoverTrackColor": "bgPanel"
      }
    },

    "Button": {
      "foreground": "textDefault",
      "startBorderColor": "bgEditor",
      "endBorderColor": "bgEditor",
      "startBackground": "bgEditor",
      "endBackground": "bgEditor",
      "focusedBorderColor": "bgEditor",
      "disabledBorderColor": "textInactive",
      "disabledText": "textInactive",
      "shadowColor": "border",
      "default": {
        "foreground": "textBtn",
        "startBackground": "secondary",
        "endBackground": "secondary",
        "startBorderColor": "secondary",
        "endBorderColor": "secondary",
        "focusColor": "secondary",
        "focusedBorderColor": "secondary",
        "shadowColor": "border"
      }
    },

    "Link": {
      "activeForeground": "primary",
      "hoverForeground": "primary",
      "visitedForeground": "primary",
      "pressedForeground": "primary"
    },

    "ProgressBar": {
      "failedColor": "#ff0000",
      "failedEndColor": "bgEditor",
      "trackColor": "bgEditor",
      "progressColor": "primary",
      "indeterminateStartColor": "secondary",
      "indeterminateEndColor": "bgEditor",
      "passedColor": "#00ff00",
      "passedEndColor": "bgEditor"
    },

    "MemoryIndicator": {
      "allocatedBackground": "bgPanel",
      "usedBackground": "bgEditor"
    }
  }
}
